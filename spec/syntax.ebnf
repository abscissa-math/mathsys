program         ::=     declaration*
declaration     ::=     KEYWORD? IDENTIFIER EQUALITY NUMBER

KEYWORD         ::=     "[A-Z][a-z]{2}(&[a-z]+)?"
IDENTIFIER      ::=     "[A-Za-z]*"
EQUALITY        ::=     "="
NUMBER          ::=     "[-+]?[0-9]+(\.[0-9]+)?"
NEWLINE         ::=     "\n+"

SPACE           ::=     " +"
MISMATCH        ::=     "."